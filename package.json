{
  "name": "sd-0x-store-manager",
  "version": "1.0.0",
  "description": "Simple CRUD to sales and products",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "test": "NODE_ENV=test nyc --all --include src/models --include src/services --include src/controllers mocha tests/unit/**/*.js --exit",
    "start": "NODE_ENV=production node src/index.js",
    "debug": "NODE_ENV=development nodemon --ignore coverage src/index.js",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json --ext .js, .jsx src",
    "migration:development": "NODE_ENV=development node -e \"require('./tests/utils/database').runMigration()\"",
    "seed:development": "NODE_ENV=development node -e \"require('./tests/utils/database').runSeed()\"",
    "migration:test": "NODE_ENV=test node -e \"require('./tests/utils/database').runMigration()\"",
    "seed:test": "NODE_ENV=test node -e \"require('./tests/utils/database').runSeed()\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tryber/sd-02-project-store-manager.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tryber/sd-02-project-store-manager/issues"
  },
  "homepage": "https://github.com/tryber/sd-02-project-store-manager#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "mysql2": "^2.3.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint-config-trybe-backend": "^1.0.4",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^11.1.1"
  }
}
